  64   2              {
  65   3                  return _TRUE;
  66   3              }
  67   2      
  68   2              if(usPreviousTime != g_usTimerCounter)
  69   2              {
  70   3                  usTimeElapsed++;
  71   3                  usPreviousTime = g_usTimerCounter;
  72   3              }
  73   2          }
  74   1          while(usTimeElapsed <= usTimeout);
