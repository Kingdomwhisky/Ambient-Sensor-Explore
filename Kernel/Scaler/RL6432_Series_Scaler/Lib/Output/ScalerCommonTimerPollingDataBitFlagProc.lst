  54          bit ScalerTimerPollingDataBitFlagProc(WORD usTimeout, WORD usRegister, BYTE ucValue, BYTE ucBit, bit bSucc
             -ess)
  55          {
  56   1          WORD usTimeElapsed = 0;
  57   1          WORD usPreviousTime = 0;
  58   1      
  59   1          usPreviousTime = g_usTimerCounter;
  60   1      
  61   1          do
