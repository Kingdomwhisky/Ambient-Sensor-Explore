  57          void ScalerOsdWriteProtection(bit bEnable, EnumOsdWriteType enumOsdWriteType, BYTE ucBitSelect)
  58          {
  59   1          BYTE ucTemp = 0;
  60   1      
  61   1          // Set OSD Circuit Enable for Write-Protection Function effective in OSD enable.
  62   1          if((bEnable == _ENABLE) && (ScalerGetBit(P3A_02_FRAME_CTRL_02, _BIT0) == 0x00))
  63   1          {
  64   2              if(ScalerOsdGetOsdEnable() == _TRUE)
  65   2              {
  66   3                  ScalerOsdDisableOsd();
  67   3              }
  68   2      
  69   2              ScalerOsdCircuitActivated(_ENABLE);
  70   2          }
  71   1      
  72   1          ScalerTimerWaitForEvent(_EVENT_DVS);
  73   1      
  74   1          if(enumOsdWriteType == _OSD_BYTE1)
  75   1          {
  76   2              ucTemp |= _BIT1;
  77   2          }
  78   1          else if(enumOsdWriteType == _OSD_BYTE2)
  79   1          {
  80   2              ucTemp |= _BIT2;
  81   2          }
  82   1      
  83   1          ScalerSetBit(P3A_33_FRAME_CTRL_33, ~(_BIT2 | _BIT1 | _BIT0), (ucTemp | (BYTE)bEnable));
  84   1          ScalerSetByte(P3A_34_FRAME_CTRL_34, ucBitSelect);
  85   1      }


