  51          // Description  : Load Panel Uniformity LUT
  52          // Input Value  : LUT, Bank number, EnumUniformityModeSelect
  53          // Output Value : None
  54          //--------------------------------------------------
  55          void ScalerColorPanelUniformityOffsetAdjust(BYTE *pucUNILUT, WORD usUNILUTSize, BYTE ucBankNum)
  56          {
  57   1          // Enable uniformity access
  58   1          ScalerSetBit(P34_00_UN_GLOBAL_CTRL, ~_BIT2, _BIT2);
  59   1      
  60   1          // Load uniformity offset LUT
  61   1          ScalerBurstWrite(pucUNILUT, usUNILUTSize, ucBankNum, P34_0F_UN_OFFSET_LUT_ACCESS_PORT, _BURSTWRITE_DAT
             -A_COMMON, _BURSTWRITE_FROM_FLASH);
  62   1      
  63   1          // Disable uniformity access
  64   1          ScalerSetBit(P34_00_UN_GLOBAL_CTRL, ~_BIT2, 0x00);
  65   1      }
