  52          // Input Value  : ucRow, ucCol -> 1-bit font position
  53          //                ucWidth      -> font width
  54          //                ucLength     -> number of 1-bit font to be changed
  55          // Output Value :
  56          //--------------------------------------------------
  57          void ScalerOsdCharacterWidth(BYTE ucRow, BYTE ucCol, BYTE ucWidth, BYTE ucLength)
  58          {
  59   1          BYTE ucIdx = 0;
  60   1          bit bOSDOn = _OFF;
  61   1      
  62   1          // Get OSD Status for Write-Protection Function setting OSD circuit enable.
  63   1          bOSDOn = ScalerGetBit(P3A_02_FRAME_CTRL_02, _BIT0);
  64   1      
  65   1          ScalerOsdWriteProtection(_ENABLE, _OSD_BYTE0, (_BIT7 | _BIT6 | _BIT5));
  66   1          ScalerOsdSramAddressCount(ucRow, ucCol, _OSD_BYTE0);
  67   1      
  68   1          for(ucIdx = 0; ucIdx < ucLength; ucIdx++)
  69   1          {
  70   2              ScalerOsdDataPort(ucWidth);
  71   2          }
  72   1      
  73   1          ScalerOsdWriteProtection(_DISABLE, _OSD_BYTE0, (_BIT7 | _BIT6 | _BIT5));
  74   1      
  75   1          // Recovery OSD Status
  76   1          if(bOSDOn == _OFF)
  77   1          {
  78   2              ScalerOsdCircuitActivated(bOSDOn);
  79   2          }
  80   1      }
