  52          //                bLocation: 0-Write to SDRAM, 1- Write to LATCH
  53          // Output Value : None
  54          //--------------------------------------------------
  55          void ScalerColorOutputGammaChannelCtrl(BYTE ucColorChannel, WORD usOffset, bit bLocation)
  56          {
  57   1          // Select Channel
  58   1          ScalerSetBit(P0_67_GAMMA_CTRL_SETA, ~(_BIT7 | _BIT5 | _BIT4 | _BIT2), ((ucColorChannel << 4) | ((BYTE)
             -bLocation << 2)));
  59   1      
  60   1          // Specify address
  61   1          ScalerSetBit(P0_67_GAMMA_CTRL_SETA, ~_BIT3, _BIT3);
  62   1          ScalerSetByte(P0_66_GAMMA_PORT_SETA, HIBYTE(usOffset));
  63   1          ScalerSetByte(P0_66_GAMMA_PORT_SETA, LOBYTE(usOffset));
  64   1          ScalerSetBit(P0_67_GAMMA_CTRL_SETA, ~(_BIT7 | _BIT3), _BIT7);
  65   1      }
