  52          //                usLength       -> Horizontal max size
  53          //                Width          -> Vertical max size
  54          // Output Value :
  55          //--------------------------------------------------
  56          void ScalerOsdRotationWindowSize(EnumOsdMapType enumOsdMapType, WORD usLength, WORD usWidth)
  57          {
  58   1          if(enumOsdMapType == _OSD_A)
  59   1          {
  60   2              // First OSD Width
  61   2              ScalerSetBit(P3A_38_FRAME_CTRL_38, ~(_BIT3 | _BIT2 | _BIT1 | _BIT0), ((usWidth & 0x0F00) >> 8));
  62   2              ScalerSetByte(P3A_3A_FRAME_CTRL_3A, (usWidth & 0xFF));
  63   2      
  64   2              // First OSD Length
  65   2              ScalerSetBit(P3A_38_FRAME_CTRL_38, ~(_BIT7 | _BIT6 | _BIT5 | _BIT4), ((usLength & 0x0F00) >> 4));
  66   2              ScalerSetByte(P3A_39_FRAME_CTRL_39, (usLength & 0xFF));
  67   2          }
  68   1          else
  69   1          {
  70   2              // Second OSD Width
  71   2              ScalerSetBit(P3A_3B_FRAME_CTRL_3B, ~(_BIT3 | _BIT2 | _BIT1 | _BIT0), ((usWidth & 0x0F00) >> 8));
  72   2              ScalerSetByte(P3A_3D_FRAME_CTRL_3D, (usWidth & 0xFF));
  73   2      
  74   2              // Second OSD Length
  75   2              ScalerSetBit(P3A_3B_FRAME_CTRL_3B, ~(_BIT7 | _BIT6 | _BIT5 | _BIT4), ((usLength & 0x0F00) >> 4));
  76   2              ScalerSetByte(P3A_3C_FRAME_CTRL_3C, (usLength & 0xFF));
  77   2          }
  78   1      }
