  51          // Description  : Osd Load Hardware VLC
  52          // Input Value  : *pArray          -> VLC table
  53          //                usVLCTableSize   -> Use VLC_TABLE_SIZE(VLC table)
  54          //                ucBankNum        -> Bank number (GET_CURRENT_BANK_NUMBER())
  55          //                usOffset         -> Font address
  56          //                usFontTableStart -> Font base address
  57          //                ucOsdRotateType  -> _OSD_ROTATE_DEGREE_0 or _OSD_ROTATE_DEGREE_90 or _OSD_ROTATE_DEGREE_
             -270 or _OSD_ROTATE_DEGREE_180
  58          //                bTableLocation   ->
  59          // Output Value :
  60          //--------------------------------------------------
  61          void ScalerOsdHardwareCompression(BYTE *pucArray, WORD usVLCTableSize, BYTE ucBankNum, WORD usOffset, WORD
             - usFontTableStart, EnumOsdRotateType enumOsdRotateType, bit bTableLocation)
  62          {
  63   1          BYTE ucVlcType = 0x00;
  64   1          BYTE ucFFF1Backup = MCU_FFF1_SCA_WR_INTRVL;
  65   1      
  66   1          if(enumOsdRotateType == _OSD_ROTATE_DEGREE_90)
  67   1          {
  68   2              ucVlcType = _OSD_ROTATE_CW | _OSD_ROTATE_HARDWARE_ROTATION_ENABLE | _OSD_ROTATE_ROTATION;
  69   2          }
  70   1          else if(enumOsdRotateType == _OSD_ROTATE_DEGREE_270)
  71   1          {
  72   2              ucVlcType = _OSD_ROTATE_CCW | _OSD_ROTATE_HARDWARE_ROTATION_ENABLE | _OSD_ROTATE_ROTATION;
  73   2          }
  74   1          else if(enumOsdRotateType == _OSD_ROTATE_DEGREE_180)
  75   1          {
  76   2              ucVlcType = _OSD_ROTATE_CW180 | _OSD_ROTATE_HARDWARE_ROTATION_ENABLE | _OSD_ROTATE_NORMAL;
  77   2          }
  78   1          else
  79   1          {
  80   2              ucVlcType = _OSD_ROTATE_NO_SWAP;
  81   2          }
  82   1      
  83   1          // rotation
  84   1          ScalerSetBit(P3A_0B_FRAME_CTRL_0B, ~(_BIT7 | _BIT6 | _BIT2 | _BIT0), ucVlcType);
  85   1      
  86   1          // osd compression enable
  87   1          if(enumOsdRotateType != _OSD_ROTATE_DEGREE_0)
  88   1          {
  89   2              ScalerSetBit(P3A_17_FRAME_CTRL_17, ~(_BIT1 | _BIT0), (_OSD_DECODE_NON_COMPRESSED | _OSD_COMPRESSIO
             -N_ENABLE));
  90   2          }
  91   1          else
  92   1          {
  93   2              ScalerSetBit(P3A_17_FRAME_CTRL_17, ~(_BIT1 | _BIT0), (_OSD_DECODE_NON_COMPRESSED | _OSD_COMPRESSIO
             -N_DISABLE));
  94   2          }
  95   1      
  96   1          usFontTableStart = usFontTableStart + usOffset * 9;
  97   1      
  98   1          ScalerOsdSramAddressSet(usFontTableStart, _OSD_BYTEALL);
  99   1      
 100   1          MCU_FFF1_SCA_WR_INTRVL = ((ucFFF1Backup & (~(0x8F))) | (_BIT7 | _BIT1));
 101   1      
 102   1          ScalerOsdBurstWriteDataPort(pucArray, usVLCTableSize, ucBankNum, _BURSTWRITE_DATA_OSD, bTableLocation)
             -;
 103   1      
 104   1          // OSD HW needs time to process it and then write decompressed data into SRAM.
 105   1          // The value 1ms is enough at all.
 106   1          ScalerTimerDelayXms(2);
 107   1      
C51 COMPILER V8.12   RL6432_SERIES_OSDHARDWARECOMPRESSION                                  05/25/2022 15:41:38 PAGE 3   

 108   1          MCU_FFF1_SCA_WR_INTRVL = ucFFF1Backup;
 109   1      
 110   1          // osd compression disable
 111   1          ScalerSetBit(P3A_17_FRAME_CTRL_17, ~(_BIT1 | _BIT0), (_OSD_DECODE_COMPRESSED | _OSD_COMPRESSION_DISABL
             -E));
 112   1      
 113   1          switch(enumOsdRotateType)
 114   1          {
 115   2              case _OSD_ROTATE_DEGREE_90:
 116   2              case _OSD_ROTATE_DEGREE_270:
 117   2                  ScalerSetBit(P3A_0B_FRAME_CTRL_0B, ~(_BIT7 | _BIT6 | _BIT2 | _BIT0), _OSD_ROTATE_ROTATION);
 118   2                  break;
 119   2      
 120   2              default:
 121   2              case _OSD_ROTATE_DEGREE_180:
 122   2                  ScalerSetBit(P3A_0B_FRAME_CTRL_0B, ~(_BIT7 | _BIT6 | _BIT2 | _BIT0), _OSD_ROTATE_NORMAL);
 123   2                  break;
