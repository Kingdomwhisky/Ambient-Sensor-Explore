  51          // Description  : Set specific color blending
C51 COMPILER V8.12   OSDSPECIFICCOLORBLENDING                                              05/25/2022 15:41:11 PAGE 2   

  52          // Input Value  :     bEnable -> _ENABLE or _DISABLE
  53          //                    ucIndex -> BlendingColor LUT index (0~63)
  54          // Output Value :
  55          //--------------------------------------------------
  56          void ScalerOsdSpecificColorBlending(bit bEnable, BYTE ucIndex)
  57          {
  58   1          // Set Match blending type 2 color bit[5:0]
  59   1          ScalerSetBit(P3A_08_FRAME_CTRL_08, ~(_BIT4), (_BIT4));
  60   1      
  61   1          // Set Specific Color Blending (blending Type 2) Enable/Disable
  62   1          ScalerSetBit(P3A_09_FRAME_CTRL_09, ~_BIT7, ((BYTE)bEnable << 7));
  63   1      
  64   1          // Set Blending Color from 64-color LUT [5:0] (blending Type 2)
  65   1          ScalerSetBit(P3A_08_FRAME_CTRL_08, ~(_BIT3 | _BIT2), ((ucIndex & (_BIT5 | _BIT4)) >> 2));
  66   1          ScalerSetBit(P3A_09_FRAME_CTRL_09, ~(_BIT3 | _BIT2 | _BIT1 | _BIT0), (ucIndex & 0x0F));
  67   1      }
