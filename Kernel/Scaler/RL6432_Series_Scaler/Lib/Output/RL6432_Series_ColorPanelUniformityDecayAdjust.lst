  50          //--------------------------------------------------
  51          // Description  : Load Panel Uniformity LUT
  52          // Input Value  : LUT, Bank number, EnumUniformityModeSelect
  53          // Output Value : None
  54          //--------------------------------------------------
  55          void ScalerColorPanelUniformityDecayAdjust(BYTE *pucUNILUT, WORD usUNILUTSize, BYTE ucBankNum, EnumUniform
             -ityModeSelect enumUniformityMode, BYTE ucDecayDegree)
  56          {
  57   1          // Set degree of decay
  58   1          if(enumUniformityMode == _UNIFORMITY_PCM_MODE)
  59   1          {
  60   2              ScalerSetBit(P34_01_UN_MULTI_LUT_CTRL, ~(_BIT5 | _BIT4), ((ucDecayDegree / 2) << 4));
  61   2          }
  62   1          else
  63   1          {
  64   2              // Enable uniformity access
  65   2              ScalerSetBit(P34_00_UN_GLOBAL_CTRL, ~_BIT2, _BIT2);
  66   2      
  67   2              // Load uniformity gain LUT
  68   2              ScalerBurstWrite(pucUNILUT, usUNILUTSize, ucBankNum, P34_14_UN_DECAY_LUT_ACCESS_PORT, _BURSTWRITE_
             -DATA_COMMON, _BURSTWRITE_FROM_FLASH);
  69   2      
  70   2              // Disable uniformity access
  71   2              ScalerSetBit(P34_00_UN_GLOBAL_CTRL, ~_BIT2, 0x00);
  72   2          }
  73   1      }
