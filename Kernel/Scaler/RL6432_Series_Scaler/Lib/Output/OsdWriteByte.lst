  52          // Input Value  : ucIndicateAndWriteByte -> (_OSD_WIN_CONTROL or _OSD_SRAM)  |
  53          //                                          (_OSD_BYTE0       or _OSD_BYTE1       or _OSD_BYTE2       or
  54          //                                           _OSD_BYTE0_BYTE1 or _OSD_BYTE0_BYTE2 or _OSD_BYTE1_BYTE2 or _
             -OSD_BYTEALL)
  55          //                usAddr                 -> SRAM address
  56          //                ucData                 -> ucIndicate's Data
  57          // Output Value :
  58          //--------------------------------------------------
  59          void ScalerOsdWriteByte(BYTE ucIndicateAndWriteByte, EnumOsdDBufType enumDBuf, WORD usAddr, BYTE ucData)
  60          {
  61   1          enumDBuf = enumDBuf;
  62   1      
  63   1          // Set Address Indicator: 00: Window control registers, 10: Font Select and font map SRAM
  64   1          ScalerSetBit(P0_94_OSD_WRITE_OPTION, ~(_BIT3 | _BIT2), (ucIndicateAndWriteByte & (_BIT3 | _BIT2)));
  65   1      
  66   1          // Select Which Byte to Write:  001: Byte0, 010: Byte1, 100: Byte2, 011: Byte0 & Byte1, 101: Byte0 & B
             -yte2, 110: Byte1 & Byte2,111: All
  67   1          ScalerSetBit(P0_94_OSD_WRITE_OPTION, ~(_BIT7 | _BIT6 | _BIT5), (ucIndicateAndWriteByte & (_BIT7 | _BIT
             -6 | _BIT5)));
  68   1      
  69   1          // Set Address
  70   1          ScalerSetBit(P0_90_OSD_ADR_PORT_MSB, ~(_BIT6 | _BIT5 | _BIT4 | _BIT3 | _BIT2 | _BIT1 | _BIT0), (HIBYTE
             -(usAddr) & 0x7F));
  71   1          ScalerSetByte(P0_91_OSD_ADR_PORT_LSB, LOBYTE(usAddr));
  72   1      
  73   1          // Set Data
  74   1          ScalerSetByte(P0_92_OSD_DATA_PORT, ucData);
  75   1      }


