  51          // Description  : Set window extension gradient level for R/G/B channel
  52          // Input Value  : enumOsdWindowsType -> _OSD_WINDOW_0, _OSD_WINDOW_5, _OSD_WINDOW_6, _OSD_WINDOW_7, _OSD_W
             -INDOW_8, _OSD_WINDOW_9
  53          //                ucLevelR           -> window RED extension gradient level (0 ~ 255)
  54          //                ucLevelG           -> window GRN extension gradient level (0 ~ 255)
  55          //                ucLevelB           -> window BLU extension gradient level (0 ~ 255)
  56          // Output Value :
  57          //--------------------------------------------------
  58          void ScalerOsdWindowGradientLevel(EnumOsdWindowsType enumOsdWindowsType, BYTE ucLevelR, BYTE ucLevelG, BYT
             -E ucLevelB)
  59          {
  60   1          WORD usTempWindowNumber = 0;
  61   1      
  62   1          usTempWindowNumber = 0x174 + (enumOsdWindowsType * 7);
  63   1          ScalerOsdWriteAllByte((_OSD_WIN_CONTROL), usTempWindowNumber, ucLevelR, ucLevelG, ucLevelB);
