  62   1      
  63   1          usTempWindowNumber = 0x150 + (enumOsdWindowsType * 2);
  64   1          ScalerOsdWriteAllByte((_OSD_WIN_CONTROL), usTempWindowNumber, ucColorR, ucColorG, ucColorB);
