  51          // Description  : Set precise gradient level
  52          // Input Value  : bEnable            -> _ENABLE or _DISABLE
  53          //                enumOsdWindowsType -> _OSD_WINDOW_0, _OSD_WINDOW_5, _OSD_WINDOW_6, _OSD_WINDOW_7, _OSD_W
             -INDOW_8, _OSD_WINDOW_9
  54          //                ucPixelStep        -> Pixel Step for toggle/incremental, can not be 0 (1 ~ 255)
  55          //                ucDecimalPart      -> Decimal part for Pixel-step (0~7)
  56          // Output Value :
  57          //--------------------------------------------------
  58          void ScalerOsdWindowGrayLevel(bit bEnable, EnumOsdWindowsType enumOsdWindowsType, BYTE ucPixelStep, BYTE u
             -cDecimalPart)
  59          {
  60   1          WORD usTempWindowNumber = 0;
  61   1          BYTE ucTemp = 0;
  62   1      
  63   1          usTempWindowNumber = 0x177 + (enumOsdWindowsType * 7);
  64   1      
  65   1          ucTemp = ScalerOsdFrameControlByteGet(usTempWindowNumber, _OSD_BYTE0);
  66   1          ucTemp = ((ucTemp & ~_BIT1) | ((BYTE)bEnable << 1));
  67   1      
  68   1          ScalerOsdFrameControlAllByte(usTempWindowNumber, _OSD_WITHOUT_DB, ucTemp, ucPixelStep, ucDecimalPart);
  69   1      }
