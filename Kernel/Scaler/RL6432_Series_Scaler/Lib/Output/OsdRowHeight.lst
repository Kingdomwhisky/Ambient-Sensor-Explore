  68   1          if(ScalerGetBit(P3A_18_FRAME_CTRL_18, _BIT3) == _BIT3) // OSD 2 font function
  69   1          {
  70   2              if(ucRow >= _OSD_MAP_B_ROW_OFFSET)
  71   2              {
  72   3                  ScalerOsdSramAddressSet((ScalerGetByte(P3A_1A_FRAME_CTRL_1A) + (ucRow - _OSD_MAP_B_ROW_OFFSET)
             -), _OSD_BYTE1);
  73   3              }
  74   2              else
  75   2              {
  76   3                  ScalerOsdSramAddressSet((ScalerGetByte(P3A_19_FRAME_CTRL_19) + ucRow), _OSD_BYTE1);
  77   3              }
  78   2          }
  79   1          else
  80   1          {
  81   2              ScalerOsdSramAddressSet((0 + ucRow), _OSD_BYTE1);
  82   2          }
  83   1      
