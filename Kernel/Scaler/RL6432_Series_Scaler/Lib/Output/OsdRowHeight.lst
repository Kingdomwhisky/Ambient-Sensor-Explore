  57          void ScalerOsdRowHeight(BYTE ucRow, BYTE ucHeight, BYTE ucLength)
  58          {
  59   1          BYTE ucIdx = 0;
  60   1      
  61   1          if(ucHeight > 64)
  62   1          {
  63   2              ucHeight = 64;
  64   2          }
  65   1      
  66   1          ucHeight--;
