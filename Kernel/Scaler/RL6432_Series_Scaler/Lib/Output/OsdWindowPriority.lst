  51          // Description  : Set window Priority
  52          // Input Value  : enumOsdWindowsType        -> _OSD_WINDOW_2 or _OSD_WINDOW_3 or _OSD_WINDOW_4 or _OSD_WIN
             -DOW_8 or _OSD_WINDOW_9
  53          //                enumOsdWindowPriorityType -> _OSD_WINDOW_PRIORITY_LOWER_THAN_FONT or _OSD_WINDOW_PRIORIT
             -Y_HIGHTER_THAN_FONT
  54          // Output Value :
  55          //--------------------------------------------------
  56          void ScalerOsdWindowPriority(EnumOsdWindowsType enumOsdWindowsType, EnumOsdWindowPriorityType enumOsdWindo
             -wPriorityType)
  57          {
  58   1          if(enumOsdWindowsType == _OSD_WINDOW_2)
  59   1          {
  60   2              ScalerSetBit(P3A_08_FRAME_CTRL_08, ~_BIT7, (enumOsdWindowPriorityType << 7));
  61   2          }
  62   1          else if(enumOsdWindowsType == _OSD_WINDOW_3)
  63   1          {
  64   2              ScalerSetBit(P3A_07_FRAME_CTRL_07, ~_BIT0, enumOsdWindowPriorityType);
  65   2          }
  66   1          else if(enumOsdWindowsType == _OSD_WINDOW_4)
  67   1          {
  68   2              ScalerSetBit(P3A_07_FRAME_CTRL_07, ~_BIT1, (enumOsdWindowPriorityType << 1));
  69   2          }
  70   1          else if(enumOsdWindowsType == _OSD_WINDOW_8)
  71   1          {
  72   2              ScalerSetBit(P3A_08_FRAME_CTRL_08, ~_BIT5, (enumOsdWindowPriorityType << 5));
  73   2          }
  74   1          else if(enumOsdWindowsType == _OSD_WINDOW_9)
  75   1          {
  76   2              ScalerSetBit(P3A_08_FRAME_CTRL_08, ~_BIT6, (enumOsdWindowPriorityType << 6));
  77   2          }
  78   1      }
