  58          void ScalerOsdSetColorPalette(BYTE ucIndex, BYTE ucColorR, BYTE ucColorG, BYTE ucColorB)
  59          {
  60   1          ScalerSetByte(P0_6E_OVERLAY_LUT_ADDR, (_BIT7 | ucIndex));
  61   1          ScalerSetByte(P0_6F_COLOR_LUT_PORT, ucColorR);
  62   1          ScalerSetByte(P0_6F_COLOR_LUT_PORT, ucColorG);
  63   1          ScalerSetByte(P0_6F_COLOR_LUT_PORT, ucColorB);
  64   1          ScalerSetByte(P0_6E_OVERLAY_LUT_ADDR, 0x00);
