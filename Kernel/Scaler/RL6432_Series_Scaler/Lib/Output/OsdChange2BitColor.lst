  51          // Description  : SRAM address value and which byte
  52          // Input Value  : ucRow. ucCol. ucLength. ucHeight.       -> Just that !!
  53          //                ucColor0. ucColor1. ucColor2. ucColor3  -> Color number
  54          // Output Value :
  55          //--------------------------------------------------
  56          void ScalerOsdChange2BitColor(BYTE ucRow, BYTE ucCol, BYTE ucLength, BYTE ucHeight, BYTE ucColor0, BYTE uc
             -Color1, BYTE ucColor2, BYTE ucColor3)
  57          {
  58   1          BYTE ucTemp = 0;
  59   1          BYTE ucByte0 = _BIT7;
  60   1          BYTE ucByte2 = 0;
  61   1      
  62   1          if((ucColor0 > 7) && (ucColor3 > 7))
  63   1          {
  64   2              ucByte0 |= _BIT5;
  65   2              ucColor0 -= 8;
  66   2              ucColor3 -= 8;
  67   2          }
  68   1      
  69   1          if((ucColor1 > 7) && (ucColor2 > 7))
  70   1          {
  71   2              ucByte0 |= _BIT4;
  72   2              ucColor1 -= 8;
  73   2              ucColor2 -= 8;
  74   2          }
  75   1      
  76   1          ucByte0 = ucByte0 | ((ucColor3 & 0x07) << 1) | ((ucColor0 & _BIT2) >> 2);
  77   1          ucByte2 = ((ucColor0 & 0x03) << 6) | ((ucColor2 & 0x07) << 3) | (ucColor1 & 0x07);
  78   1      
  79   1          for(ucTemp = 0; ucTemp < ucHeight; ucTemp++)
  80   1          {
  81   2              ScalerOsdSramDataValue2Byte(ucRow + ucTemp, ucCol, ucLength, ucByte0, ucByte2, _OSD_BYTE0_BYTE2);
  82   2          }
  83   1      }
