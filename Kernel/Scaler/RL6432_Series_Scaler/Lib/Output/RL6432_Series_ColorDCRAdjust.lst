  55          void ScalerColorDCRAdjust(WORD usThreshold1, WORD usThreshold2, EnumDCRMeasureSel enumDCRMeasureSel)
  56          {
  57   1          ScalerSetByte(P7_D8_DCR_ADDRESS_PORT, (_P7_D9_PT_00_DCR_THESHOLD1_1 << 2) | _BIT0);
  58   1          ScalerSetByte(P7_D9_DCR_DATA_PORT, usThreshold1 >> 2);
  59   1          ScalerSetByte(P7_D8_DCR_ADDRESS_PORT, (_P7_D9_PT_01_DCR_THESHOLD2_1 << 2) | _BIT0);
  60   1          ScalerSetByte(P7_D9_DCR_DATA_PORT, usThreshold2 >> 2);
  61   1      
  62   1          ScalerSetByte(P7_D8_DCR_ADDRESS_PORT, (_P7_D9_PT_38_DCR_THESHOLD1_0 << 2) | _BIT0);
  63   1          ScalerSetByte(P7_D9_DCR_DATA_PORT, (usThreshold1 & (_BIT1 | _BIT0)) << 6);
  64   1          ScalerSetByte(P7_D8_DCR_ADDRESS_PORT, (_P7_D9_PT_39_DCR_THESHOLD2_0 << 2) | _BIT0);
  65   1          ScalerSetByte(P7_D9_DCR_DATA_PORT, (usThreshold2 & (_BIT1 | _BIT0)) << 6);
  66   1      
  67   1          ScalerSetBit(P7_DA_DCR_CTRL, ~(_BIT3 | _BIT2 | _BIT1), (enumDCRMeasureSel << 1));
  68   1      }
