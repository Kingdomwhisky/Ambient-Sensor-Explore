C51 COMPILER V8.12   OSDFRAMECONTROLBYTEGET                                                05/25/2022 15:41:08 PAGE 2   

  52          // Input Value  : usAddr           -> WindowControl address
  53          //                enumOsdWriteType -> _OSD_BYTE0 or _OSD_BYTE1 or _OSD_BYTE2
  54          // Output Value : FrameControl Value
  55          //--------------------------------------------------
  56          BYTE ScalerOsdFrameControlByteGet(WORD usAddr, EnumOsdWriteType enumOsdWriteType)
  57          {
  58   1          ScalerSetBit(P0_94_OSD_WRITE_OPTION, ~(_BIT7 | _BIT6 | _BIT5), enumOsdWriteType);
  59   1          ScalerSetBit(P0_94_OSD_WRITE_OPTION, ~(_BIT3 | _BIT2), _OSD_WIN_CONTROL);
  60   1      
  61   1          ScalerSetByte(P0_90_OSD_ADR_PORT_MSB, (HIBYTE(usAddr) & 0x7F));
  62   1          ScalerSetByte(P0_91_OSD_ADR_PORT_LSB, LOBYTE(usAddr));
  63   1      
  64   1          return ScalerGetByte(P0_92_OSD_DATA_PORT);
  65   1      }


