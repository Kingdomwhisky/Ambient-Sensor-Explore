  57          void ScalerOsdFontRotateCtrl(EnumOsdRotateType enumRotType, BYTE ucFontDownloadRotEn, BYTE ucDispRotEn)
  58          {
  59   1          switch(enumRotType)
  60   1          {
  61   2              default:
  62   2              case _OSD_ROTATE_DEGREE_0:
  63   2                  ScalerSetBit(P3A_0B_FRAME_CTRL_0B, ~(_BIT7 | _BIT6), 0x00);
  64   2                  break;
  65   2      
  66   2              case _OSD_ROTATE_DEGREE_90:
  67   2                  ScalerSetBit(P3A_0B_FRAME_CTRL_0B, ~(_BIT7 | _BIT6), (_BIT7 | _BIT6));
