  75   2                  ScalerSetBit(P3A_0B_FRAME_CTRL_0B, ~(_BIT7 | _BIT6), _BIT6);
  76   2                  break;
  77   2          }
  78   1      
  79   1          if(((bit)ucFontDownloadRotEn) == _ENABLE)
  80   1          {
  81   2              ScalerSetBit(P3A_0B_FRAME_CTRL_0B, ~_BIT2, _BIT2);
  82   2          }
  83   1          else
  84   1          {
  85   2              ScalerSetBit(P3A_0B_FRAME_CTRL_0B, ~_BIT2, 0x00);
  86   2          }
  87   1      
  88   1          if((enumRotType == _OSD_ROTATE_DEGREE_0) || (enumRotType == _OSD_ROTATE_DEGREE_180))
  89   1          {
  90   2              ScalerSetBit(P3A_0B_FRAME_CTRL_0B, ~_BIT0, 0x00);
  91   2          }
  92   1          else
  93   1          {
  94   2              ScalerSetBit(P3A_0B_FRAME_CTRL_0B, ~_BIT0, (bit)ucDispRotEn);
  95   2          }
  96   1      }
