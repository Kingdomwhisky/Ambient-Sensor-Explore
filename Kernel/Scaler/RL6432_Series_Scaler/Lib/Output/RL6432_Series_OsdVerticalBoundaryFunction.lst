  52          // Description  : Set vertical boundary function
  53          // Input Value  : bEnable   -> _ENABLE or _DISABLE
  54          // Output Value :
  55          //--------------------------------------------------
  56          void ScalerOsdVerticalBoundary(bit bEnable)
  57          {
  58   1          ScalerSetBit(P3A_18_FRAME_CTRL_18, ~_BIT5, ((BYTE)bEnable << 5));
  59   1      }
  60          
  61          
  62          /*
  63          //--------------------------------------------------
  64          // example
  65          //--------------------------------------------------
  66              ScalerOsdVerticalBoundary(_ENABLE);
  67          */
  68          
  69          
  70          //--------------------------------------------------
  71          // Description  : Set vertical boundary size
  72          // Input Value  : usUpperBoundary -> 0 ~ 2047
  73          //                usLowerBoundary -> 0 ~ 2047
  74          // Output Value :
  75          //--------------------------------------------------
  76          void ScalerOsdVerticalBoundarySize(WORD usUpperBoundary, WORD usLowerBoundary)
  77          {
  78   1          ScalerSetBit(P3A_1E_FRAME_CTRL_1E, ~(_BIT6 | _BIT5 | _BIT4), ((HIBYTE(usUpperBoundary) & 0x07) << 4));
             -   // OSD Vertical Upper Boundary [10:8]
  79   1          ScalerSetBit(P3A_1E_FRAME_CTRL_1E, ~(_BIT2 | _BIT1 | _BIT0), (HIBYTE(usLowerBoundary) & 0x07));       
             -   // OSD Vertical Lower Boundary [10:8]
  80   1      
  81   1          ScalerSetByte(P3A_1F_FRAME_CTRL_1F, LOBYTE(usUpperBoundary)); // OSD Vertical Upper Boundary [7:0]
  82   1          ScalerSetByte(P3A_20_FRAME_CTRL_20, LOBYTE(usLowerBoundary)); // OSD Vertical Lower Boundary [7:0]
  83   1      }
  84          
  85          
  86          /*
  87          //--------------------------------------------------
  88          // example
  89          //--------------------------------------------------
  90              ScalerOsdVerticalBoundarySize(YSTART(100), YEND(1000));
  91          */
  92          


