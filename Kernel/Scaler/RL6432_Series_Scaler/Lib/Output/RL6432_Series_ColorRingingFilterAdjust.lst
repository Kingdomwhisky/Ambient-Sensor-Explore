  60          void ScalerColorRingingFilterAdjust(BYTE *pucOffsetCoef)
  61          {
  62   1          BYTE ucTHD = 0;
  63   1          BYTE ucRFEn = 0;
  64   1      
  65   1          if(*pucOffsetCoef > 0)
  66   1          {
  67   2              ucTHD = (WORD)_RINGING_FILTER_DELTA * 128 / (*pucOffsetCoef);
  68   2          }
  69   1          else
  70   1          {
  71   2              ucTHD = 255;
  72   2          }
  73   1      
  74   1          if(ucTHD < _RINGING_FILTER_THRESHOLD)
  75   1          {
  76   2              ucTHD = _RINGING_FILTER_THRESHOLD;
  77   2          }
