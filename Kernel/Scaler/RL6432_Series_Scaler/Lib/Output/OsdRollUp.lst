  52          // Input Value  :  bEnable    -> _ENABLE or _DISABLE
  53          //                 ucRowStart -> Set RollUp start (end) row
  54          //                 ucRowBack  -> Set RollUp reposition row
  55          // Output Value :
  56          //--------------------------------------------------
  57          void ScalerOsdRollUp(bit bEnable, BYTE ucRowStart, BYTE ucRowBack)
  58          {
  59   1          BYTE pucTemp[2];
  60   1          WORD usCharCommandAddress = 0;
  61   1          memset(pucTemp, 0, sizeof(pucTemp));
  62   1      
  63   1          // Set OSD Special Function Enable/Disable
  64   1          ScalerSetBit(P3A_18_FRAME_CTRL_18, ~(_BIT7 | _BIT6), ((BYTE)bEnable << 7));
  65   1      
  66   1          // Get Font Select Base Address
  67   1          pucTemp[0] = ScalerGetByte(P3A_0C_FRAME_CTRL_0C);
  68   1          pucTemp[1] = ScalerGetByte(P3A_0D_FRAME_CTRL_0D);
  69   1          usCharCommandAddress = (WORD)((pucTemp[1] & 0xF0) << 4) | pucTemp[0];
  70   1          usCharCommandAddress = usCharCommandAddress + (ucRowStart * GET_OSD_MAPA_COL_MAX());
  71   1      
  72   1          // Set Row Command Base 0
  73   1          ScalerSetByte(P3A_19_FRAME_CTRL_19, ucRowStart);
  74   1          // Set Row Command Base 1
  75   1          ScalerSetByte(P3A_1A_FRAME_CTRL_1A, ucRowBack);
  76   1      
  77   1          // Set Font Select Base 0
  78   1          ScalerSetBit(P3A_1B_FRAME_CTRL_1B, ~(_BIT7 | _BIT6 | _BIT5 | _BIT4), ((usCharCommandAddress & 0xF00) >
             -> 4));
  79   1          ScalerSetByte(P3A_1C_FRAME_CTRL_1C, (usCharCommandAddress & 0x0FF));
  80   1      }


