  52          // Input Value  : bEnable    -> _ENABLE or _DISABLE
  53          //                enumOsdWindowsType -> _OSD_WINDOW_0, _OSD_WINDOW_5, _OSD_WINDOW_6, _OSD_WINDOW_7, _OSD_W
             -INDOW_8, _OSD_WINDOW_9
  54          //                ucStep           -> extended gradient step (0 ~ 255)
  55          // Output Value :
  56          //--------------------------------------------------
  57          void ScalerOsdWindowGradientStep(bit bEnable, EnumOsdWindowsType enumOsdWindowsType, BYTE ucStep)
  58          {
  59   1          WORD usTempWindowNumber = 0;
  60   1          BYTE ucTemp = 0;
  61   1      
  62   1          // Set Extended Gradient Step
  63   1          usTempWindowNumber = 0x176 + (enumOsdWindowsType * 7);
  64   1          ScalerOsdFrameControlByte(usTempWindowNumber, _OSD_BYTE1, ucStep);
  65   1      
  66   1          // Set Window Gradient Step Extension Enable/Disable
  67   1          usTempWindowNumber++;
  68   1          ucTemp = ScalerOsdFrameControlByteGet(usTempWindowNumber, _OSD_BYTE0);
  69   1          ucTemp = ((ucTemp & ~_BIT4) | ((BYTE)bEnable << 4));
  70   1          ScalerOsdFrameControlByte(usTempWindowNumber, _OSD_BYTE0, ucTemp);
  71   1      }
