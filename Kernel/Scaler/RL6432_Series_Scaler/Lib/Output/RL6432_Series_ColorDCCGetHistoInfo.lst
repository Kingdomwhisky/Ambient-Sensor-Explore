  54          void ScalerColorDCCGetHistoInfo(BYTE *pucReadArray)
  55          {
  56   1          BYTE ucI = 0;
  57   1      
  58   1          if(GET_DCC_READY_STATUS() == _FALSE)
  59   1          {
  60   2              memset(pucReadArray, 0, 11);
  61   2              return;
  62   2          }
  63   1      
  64   1          ScalerSetBit(P7_C7_DCC_CTRL_0, ~(_BIT1 | _BIT0), _PAGE0);
  65   1      
  66   1          // Pop out result
  67   1          ScalerSetByte(P7_C9_DCC_ADDRESS_PORT, _P7_CA_PT_0C_PORT_PAGE0_POPUP_CTRL);
  68   1          // Read 0x07C9_0C to freeze DCC infomation
  69   1          ScalerGetByte(P7_CA_DCC_DATA_PORT);
  70   1      
  71   1          // Get Histogram Info
  72   1          ScalerSetByte(P7_C9_DCC_ADDRESS_PORT, _P7_CA_PT_0E_PORT_PAGE0_Y_MAX_VAL_H);
  73   1      
  74   1          for(ucI = 0; ucI < 9; ucI++)
  75   1          {
  76   2              pucReadArray[ucI] = ScalerGetByte(P7_CA_DCC_DATA_PORT);
  77   2          }
  78   1      
  79   1          ScalerSetByte(P7_C9_DCC_ADDRESS_PORT, _P7_CA_PT_1C_PORT_PAGE0_Y_MAX_VAL_L);
  80   1          pucReadArray[9] = ScalerGetByte(P7_CA_DCC_DATA_PORT);
  81   1          pucReadArray[10] = ScalerGetByte(P7_CA_DCC_DATA_PORT);
