  51          // Description  : Align vertical offset by DVBGStart
  52          // Input Value  : ucVOffset           -> Vertical offset base on DVBGStart 6
  53          // Output Value : Vertical offset compensation depend on DVBGStart
  54          //--------------------------------------------------
  55          BYTE ScalerOsdPositionOffsetVerticalAlignByDVBGStart(BYTE ucVOffset)
  56          {
  57   1          WORD usDVBGStart = 0;
  58   1      
  59   1          ScalerGetDataPortByte(P0_2A_DISPLAY_FORMAT_ADDR_PORT, _P0_2B_PT_0E_DISPLAY_VER_BG_STA_H, 2, &pData[0],
             - _AUTOINC);
  60   1          usDVBGStart = ((pData[0] & 0x1F) << 8) | (pData[1]);
  61   1      
  62   1          if((ScalerOsdGetDoubleFunctionH() == _TRUE) && (ScalerOsdGetDoubleFunctionV() == _TRUE))
  63   1          {
  64   2              if(usDVBGStart > 6)
  65   2              {
  66   3                  ucVOffset = ucVOffset + ((usDVBGStart - 6) / 2);
  67   3              }
  68   2              else
  69   2              {
  70   3                  if(ucVOffset >= ((6 - usDVBGStart + 1) / 2))
  71   3                  {
  72   4                      ucVOffset = ucVOffset - ((6 - usDVBGStart + 1) / 2);
  73   4                  }
  74   3                  else
  75   3                  {
  76   4                      ucVOffset = 0;
  77   4                  }
  78   3              }
  79   2          }
  80   1          else
  81   1          {
  82   2              if(usDVBGStart > 6)
  83   2              {
  84   3                  ucVOffset = ucVOffset + usDVBGStart - 6;
  85   3              }
  86   2              else
  87   2              {
  88   3                  if(ucVOffset >= (6 - usDVBGStart))
  89   3                  {
  90   4                      ucVOffset = ucVOffset - (6 - usDVBGStart);
  91   4                  }
  92   3                  else
  93   3                  {
  94   4                      ucVOffset = 0;
  95   4                  }
  96   3              }
  97   2          }
  98   1      
  99   1          // Check OSD V offset minimum
 100   1          if(ucVOffset < _OSD_V_OFFSET_MIN)
 101   1          {
 102   2              return _OSD_V_OFFSET_MIN;
 103   2          }
 104   1          else
 105   1          {
 106   2              return ucVOffset;
 107   2          }
 108   1      }
